{
	"manifest_version": 2.0,
	"id": "geomapmonzphere",
	"type": "widget",
	"name": "Geomap Monzphere",
	"namespace": "GeomapMonzphere",
	"version": "1.0",
	"author": "Monzphere",
	"description": "Displays hosts as markers on a geographical map.",
	"url": "monzphere.com",
	"widget": {
		"js_class": "CWidgetGeoMapMonzphere",
        "in": {
			"hostids": {
				"type": "_hostids"
			},
			"groupids": {
				"type": "_hostgroupids"
			}
		},
		"out": [
            {
				"type": "_hostid"
			},
            {
				"type": "_hostids"
			}
        ]
	},
	"actions": {
		"widget.geomapmonzphere.view": {
			"class": "WidgetView"
		}
	},
	"assets": {
		"js": ["class.widget.js"],
		"css": ["widget.css"]
	}
}
